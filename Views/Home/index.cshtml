@{
    ViewData["Title"] = "Top Bargains";
}

<table class="table">
  <thead>
    <tr>
      <th scope="col">Top Bargains</th>
    </tr>
  </thead>
  <tbody>
    @foreach (string bargain in ViewBag.bargains) {
      <tr>
    <td><p>@bargain</p></td>
    </tr>
    }
  </tbody>
</table>